{
  "expo": {
    "name": "Perfin",
    "slug": "perfin",
    "version": "1.0.0",
    "scheme": "perfin",
    "orientation": "portrait",
    "icon": "./assets/icons/adaptive-icon.png",
    "splash": {
      "image": "./assets/icons/splash-icon-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"},
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.borisatanassov.Perfin",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "monochromeImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.borisatanassov.Perfin",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a672f513-0535-4662-9516-daa2952b6f2d"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/a672f513-0535-4662-9516-daa2952b6f2d"
    }
  }
}
